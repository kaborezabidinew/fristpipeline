pipeline{
    agent any
    tools {
        maven 'M2_HOME'
    }

    stages{
        stage('sonarqube scanner') {
            steps {
                sh 'mvn sonar:sonar'
            }
        }
        stage ('All maven commands') {
            steps {
                sh 'mvn clean test compile install package'
            }
        }
        
    }
}


